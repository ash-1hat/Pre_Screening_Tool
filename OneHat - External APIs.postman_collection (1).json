		{
			"name": "Generate Auth Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.environment.set(\"onehat_access_token\", jsonData.data.accessToken);",
							"pm.environment.set(\"onehat_access_refresh\", jsonData.data.refreshToken);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "http://localhost:8080/one-hat/api/external/auth/token?grantType=authorization&code=bXVrZXNoOllXUnRhVzQ9OjQ0NQ==",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8080",
					"path": [
						"one-hat",
						"api",
						"external",
						"auth",
						"token"
					],
					"query": [
						{
							"key": "grantType",
							"value": "authorization"
						},
						{
							"key": "code",
							"value": "bXVrZXNoOllXUnRhVzQ9OjQ0NQ==",
							"description": "Base 64 encoded value of username:base 64 encoded password : hospital id"
						}
					]
				}
			},
			"response": []
		},
